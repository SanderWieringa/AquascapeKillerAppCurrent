@model IEnumerable<AquascapeThemeKillerApp.Logic_Interfaces.AquascapeModel>

@{
    ViewData["Title"] = "GetAllAquascapes";
}
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Difficulty)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Difficulty)
            </td>
            <td>
                @Html.ActionLink("AquascapeDetails", "AquascapeDetails", new { id=item.AquascapeId }) 
            </td>
        </tr>
}
    </tbody>
</table>
