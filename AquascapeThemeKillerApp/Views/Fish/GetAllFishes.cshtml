@using AquascapeThemeKillerApp.Logic_Interfaces
@model IEnumerable<AquascapeThemeKillerApp.Logic_Interfaces.FishModel>

@{
    ViewData["Title"] = "GetAllFish";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FishName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FishType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FishSize)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (FishModel item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FishName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FishType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FishSize)
            </td>
        </tr>
}
    </tbody>
</table>
